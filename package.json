{
  "name": "@qpress/cli",
  "version": "0.0.2",
  "description": "This is an extension of @qpress/core. Can be used to easily generate qpress applications and assets inside.",
  "main": "./lib/index.js",
  "scripts": {
    "start": "npm run build && npm run global",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha src/**/*.spec.ts",
    "lint": "tslint -p . --fix",
    "build": "npm run clean:some && tsc -p .",
    "global": "npm i -g && qpress --help",
    "clean:some": "rm -rf ./lib ./docs",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
    "refresh": "npm run clean:all && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm version patch && npm run build && npm publish --access public",
    "docs": "typedoc --out docs ./src"
  },
  "bin": {
    "qpress": "./bin/qpress"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/yargs": "^15.0.5",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "kleur": "^4.1.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.0.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@liftr/tscov": "^1.4.8",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.6.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.18.0",
    "typescript": "^4.0.2"
  }
}
